generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model colors {
  id String @id @map("_id")
  time Json
  guild_id String
  names Json
  colors Json
  users Json
}